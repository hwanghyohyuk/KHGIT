package test.array.premitive;

public class ArraySample {
	public void test2(){
		int[] arr = new int[10];
		int min=0,max=0;
		
		for(int i = 0; i<arr.length;i++){
			arr[i] = (int)(Math.random()*100)+1;
		}
		for(int i=0;i<arr.length;i++){
			if(min>arr[i]){
				min=arr[i];
			}
			if(max<arr[i]){
				
			}
		}
		
	}
}
