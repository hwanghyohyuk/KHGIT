package test.array.premitive;

public class ArraySample {
	public void test2(){
		int[] arr = new int[10];
		int min=100,max=0;

		for(int i=0;i<arr.length;i++){
			arr[i] = (int)(Math.random()*100)+1;
			if(min>arr[i]){
				min=arr[i];
			}
			if(max<arr[i]){
				max=arr[i];
			}
			System.out.println();
		}
		
	}
}
